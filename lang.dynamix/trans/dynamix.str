module dynamix

imports
  
  completion/completion
  pp
  outline
  analysis
  dynamix/compile

rules // Debugging
    
  debug-show-aterm:
    (node, _, _, path, project-path) -> (filename, result)
    with
      filename := <guarantee-extension(|"aterm")> path
    ; result   := node
  
  debug-show-analyzed-aterm:
    (node, _, _, path, project-path) -> (filename, result)
    with
      filename := <guarantee-extension(|"aterm")> path
    ; result   := node
